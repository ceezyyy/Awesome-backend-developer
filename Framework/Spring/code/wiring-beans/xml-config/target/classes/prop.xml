<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="compactDisc" class="com.ceezyyy.BlackCab">
        <property name="album" value="Black Cab"></property>
        <property name="artist" value="Higher Brothers"></property>
        <property name="tracks">
            <list>
                <value>711</value>
                <value>Wechat</value>
                <value>Yahh</value>
                <value>Wudidong</value>
                <value>Mine</value>
                <value>Why Not</value>
            </list>
        </property>
    </bean>

    <bean id="cdPlayer" class="com.ceezyyy.CDPlayer">
        <property name="CD" ref="compactDisc"></property>
    </bean>

</beans>